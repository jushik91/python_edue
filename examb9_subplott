import plotly.express as px
 
df = px.data.tips()
d = dict(data_frame=df, x='total_bill', y='tip')
 
fig3 = px.scatter(**d, color='sex', facet_row='time', marginal_y='box')
fig4 = px.scatter(**d, color='sex', facet_col="day", facet_row='time',
                 category_orders={'day':['Thur', 'Fri','Sat','Sun'],'time':['Lunch', 'Dinner']})
 
fig3.show()
fig4.show()
